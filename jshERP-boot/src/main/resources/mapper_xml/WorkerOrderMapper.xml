<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jsh.erp.datasource.mappers.WorkerOrderMapper">

    <select id="iPageSelect" resultType="com.jsh.erp.order.entity.WorkerOrder">
        select * from worker_order
        <where>
            <if test="order_approval_user!=null">
                order_approval_user = #{orderApprovalUser}
            </if>
        </where>
    </select>
</mapper>
